{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "node_modules/vue-meterial-design-icons/**/*"
  ],
  "ts-node": {
    "esm": true
  },
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "allowJs": true,
    "jsx": "preserve",
    "declaration": true,
    "preserveValueImports": false
  },
  "noimplicitAny": false,
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
