{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "node_modules/vue-meterial-design-icons/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "allowJs": true,
    "jsx": "preserve",
    "declaration": true
  },
  "noimplicitAny": false,
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
